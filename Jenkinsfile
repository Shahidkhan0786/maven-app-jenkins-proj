pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'building the app....'
            }
        }
      stage('dev') {
            steps {
                echo 'app is deploy in dev ....'
            }
        }
      stage('prod') {
            steps {
                echo 'app is deploying in prod...'
            }
        }
    }
}
